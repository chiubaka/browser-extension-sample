{
  "name": "chiubaka",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nx build",
    "build:affected": "nx affected --target=build",
    "build:ci": "yarn build:affected --base=$NX_BASE --head=$NX_HEAD",
    "deploy": "nx deploy $@ --configuration=production",
    "deploy:ci": "yarn deploy",
    "lint": "nx lint",
    "lint:affected": "nx affected --target=lint",
    "lint:all": "yarn lint:root && yarn lint:packages",
    "lint:ci": "yarn lint:root && yarn lint:affected --base=$NX_BASE --head=$NX_HEAD",
    "lint:fix:all": "yarn lint:fix:root; yarn lint:fix:packages",
    "lint:fix:packages": "yarn lint:packages --fix",
    "lint:fix:root": "yarn lint:root --fix",
    "lint:packages": "nx run-many --target=lint --all",
    "lint:root": "yarn eslint .",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "start": "nx serve",
    "test": "nx test",
    "test:affected": "nx affected --target=test",
    "test:ci": "yarn test:affected --ci --coverage --base=$NX_BASE --head=$NX_HEAD && yarn test:e2e:affected --base=$NX_BASE --head=$NX_HEAD",
    "test:e2e": "nx run-many --target=e2e --all",
    "test:e2e:affected": "nx affected --target=e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chiubaka/browser-extension-sample.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiubaka/browser-extension-sample/issues"
  },
  "homepage": "https://github.com/chiubaka/browser-extension-sample#readme",
  "dependencies": {
    "@chiubaka/nx-plugin": "0.2.0",
    "core-js": "^3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.1",
    "regenerator-runtime": "0.13.7"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@chiubaka/eslint-config": "^0.5.0",
    "@chiubaka/tsconfig": "^0.1.0",
    "@nrwl/cli": "15.2.4",
    "@nrwl/cypress": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/jest": "15.2.4",
    "@nrwl/react": "15.2.4",
    "@nrwl/storybook": "15.2.4",
    "@nrwl/web": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@storybook/react": "~6.5.9",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "babel-jest": "28.1.1",
    "babel-loader": "8.1.0",
    "cypress": "^11.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.1.0",
    "nx": "15.2.4",
    "prettier": "^2.8.3",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "tslib": "^2.5.0",
    "typescript": "~4.8.2",
    "url-loader": "^3.0.0"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "yarn@3.4.1"
}
